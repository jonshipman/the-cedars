{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte", "@prettier/plugin-php"],
	"pluginSearchDirs": ["."],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"htmlWhitespaceSensitivity": "ignore",
	"phpVersion": "8.0",
	"braceStyle": "1tbs"
}
